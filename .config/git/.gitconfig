[core]
	excludesfile = /Users/hoan.lac/.gitignore_global
[commit]
	template = /Users/hoan.lac/.stCommitMsg
[includeIf "gitdir:/Users/hoan.lac/OneDrive/Hoan's Data/Company/GotoAdmins/"]
  path = ~/.gitconfig-gotoadmins
[includeIf "gitdir:/Users/hoan.lac/Google Drive/My Drive/Data/Moca/"]
  path = ~/.gitconfig-moca
[user]
	name = Hoan Lac
	email = hoan.lac@outlook.com
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/Sourcetree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[filter "lfs"]
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
[push]
	autoSetupRemote = true
